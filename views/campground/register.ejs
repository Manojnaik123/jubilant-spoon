<%- layout('layout/boiler')  %> 
    
<form action="/register" method="post" class="val">
    <div class="mb-3">
        <label class="form-label" for="username">username</label><br>
        <input type="text" class="form-control" type="text" id="username" name="username"required>
    </div>
    <div class="mb-3">
        <label class="form-label" for="email">email</label><br>
        <input type="text" class="form-control"  id="email" name="email"required>
    </div>
    <div class="mb-3">
        <label class="form-label" for="password">email</label><br>
        <input type="password" class="form-control"  id="password" name="password"required>
    </div>
    
    
    <button class="btn btn-success mt-lg-2" >register</button>
</form>